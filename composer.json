{
  "name": "prgayman/larafcm",
  "description": "Laravel Firebase Cloud Messaging",
  "version": "1.0.0",
  "keywords": [
    "prgayman",
    "laravel",
    "fcm",
    "firebase",
    "larafcm"
  ],
  "homepage": "https://github.com/prgayman/larafcm",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ayman Alaiwah",
      "email": "aymanalaiwah.dev@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.2.5|^8.0",
    "illuminate/support": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
    "illuminate/notifications": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "monolog/monolog": "^1.12|^2.0"
  },
  "autoload": {
    "psr-4": {
      "Prgayman\\LaraFcm\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Prgayman\\LaraFcm\\Providers\\LaraFcmServiceProvider"
      ],
      "aliases": {
        "LaraFcm": "Prgayman\\LaraFcm\\Facades\\LaraFcm"
      }
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
